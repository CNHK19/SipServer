<Window x:Class="UccpApiErrors.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Convert UccApiErr.h to c#" Height="600" Width="700" Loaded="Window_Loaded" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="5"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" Name="textSource" VerticalAlignment="Stretch" FontFamily="Courier New" VerticalScrollBarVisibility="Visible"></TextBox>
        <TextBox Grid.Row="2" Name="textResult" VerticalAlignment="Stretch" FontFamily="Courier New" VerticalScrollBarVisibility="Visible"></TextBox>
    </Grid>
</Window>
