<Window x:Class="Messenger.Windows.Chat"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:c="clr-namespace:Messenger.Controls"
    xmlns:w="clr-namespace:Messenger.Windows" SnapsToDevicePixels="True"
    Title="AssemblyTitle - Chat"
    Left="{Binding Source={StaticResource Settings}, Path=Default.LeftChat, Mode=TwoWay}"    Top="{Binding Source={StaticResource Settings}, Path=Default.TopChat, Mode=TwoWay}"    Width="{Binding Source={StaticResource Settings}, Path=Default.WidthChat, Mode=TwoWay}"    Height="{Binding Source={StaticResource Settings}, Path=Default.HeightChat, Mode=TwoWay}"
    Loaded="Window_Loaded" Background="Transparent" Icon="/Icons/Chat.ico" x:Uid="w12">
    <TabControl Name="tabControl" Margin="0,0,0,0">
    </TabControl>
</Window>
